{% extends 'base.html.twig' %}
{% block body %}

    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h1 class="mt-5">{{ users.name }} {{ users.surname }}</h1>
                <p class="lead">Startujesz na ocenę: {{ users.excersises.grade }}</p>
                <ul class="list-unstyled">
                    <li>Oto twoje zadania: </li>
                    <li>{{ users.excersises.excersises }}</li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container">
        <h1><center>Tutaj prześlij zdjęcie swojej pracy</center></h1>
        <div class="row">
            <div class="col-8 mx-auto mt-5">
                {{ form_start(form,{'attr':{'class':'form-material m-t-40'}}) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>

{% endblock %}